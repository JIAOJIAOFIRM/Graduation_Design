<!--logs.wxml-->
<view class="container">
  <view class='card' bindtap='setbytongyong' id='card1'>
    <view class='cardtitle'>通用</view>
    <view class='rightarrow' style='display:{{display_tong}}'><image src='../img/right_arrow.png'></image></view>
    <view class='downarrow' style='display:{{display_yong}}'><image src='../img/down_arrow.png'></image></view> 
  </view>
  <view class='list' style='top:{{tops}}px'>
      <view class='li'>消息提醒</view>
      <view class='li'>多语言</view>
      <view class='li'>字体大小</view>
      <view class='li'>开启横屏模式</view>
  </view> 
  <view class='card' style='top:{{tops}}px' id='card2' bindtap='aboutusset'>
    <view class='cardtitle'>关于我们</view>
    <view class='rightarrow' style='display:{{display_about}}'><image src='../img/right_arrow.png'></image></view>
    <view class='downarrow' style='display:{{display_us}}'><image src='../img/down_arrow.png'></image></view> 
  </view>
  <view class='list' style='top:{{aboutustop}}px;z-index:-50'>
      <view class='li'>地址</view>
      <view class='li'>联系我们</view>
      <view class='li'>客服管理</view>
      <view class='li'>合作伙伴</view>
  </view>
</view>
